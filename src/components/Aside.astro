---
import LogoAuxi from '../components/icons/LogoAuxi.astro'
---

<main>
	<img
		src='/public/svgs/logoAuxiliar.svg'
		alt='logotipo de la auxiliar'
		width='80%'
		height='auto'
	/>
	<h2>aside</h2>

	<div>
		<LogoAuxi />
	</div>
</main>

<style>
	main {
		display: flex;
		flex-direction: column;
		align-items: center;
		justify-content: center;
	}
	img {
		padding: 1rem;
	}
	div {
		width: 80%;
		height: auto;
	}
</style>
